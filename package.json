{
	"name": "ownyourdata",
	"version": "0.4.1(1)",
	"private": true,
	"scripts": {
		"ra": "adb shell am broadcast -a react.native.RELOAD",
		"ta": "react-native run-android --variant=release",
		"android-beta": "cd android && ./gradlew assembleRelease",
		"adb": "adb reverse tcp:8081 tcp:8081",
		"ios": "react-native run-ios",
		"android": "react-native run-android",
		"android-linux": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
		"refresh-android": "adb shell am broadcast -a react.native.RELOAD",
		"clean": "rm ~/.rncache/* && yarn cache clean && npm cache verify && watchman watch-del-all && rm -fr $TMPDIR/react-*",
		"clean-gradle-cache": "rm -rf $HOME/.gradle/caches/",
		"clean-gradle": "cd android && ./gradlew clean && cd ..",
		"docs": "./node_modules/.bin/esdoc -c .esdoc",
		"lint": "eslint src",
		"lint-fix": "eslint -c .eslintrc src --fix",
		"link": "react-native link",
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"postinstall": "rndebugger-open",
		"beta": "cd ios && fastlane beta test_email:dev@animal.at && cd ..",
		"storybook": "storybook start -p 7007",
		"run-job": "adb shell cmd jobscheduler run -f com.ownyourdata 999",
		"log-job": "adb logcat *:S ReactNative:V ReactNativeJS:V TSBackgroundFetch:V",
		"testFlight": "cd ios && fastlane deployToFlight && cd .."
	},
	"rnpm": {
		"assets": [
			"./fonts/"
		]
	},
